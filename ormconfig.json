{
   "type":"sqlite",
   "database": "src/database/db.sqlite",
   "synchronize": false,
   "logging": false,

   "cli":{
      "migrationsDir":"./src/database/migrations",
      "entitiesDir":"./src/models"
   },

   "entities": [
      "src/models/**/*.ts"
   ],
   "migrations": [
      "./src/database/migrations/**/*.ts"
   ],
   "subscribers": [
      "src/subscriber/**/*.ts"
   ]
}